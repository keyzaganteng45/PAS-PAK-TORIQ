        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.438" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="327dp"
        android:layout_height="43dp"
        android:layout_marginStart="19dp"
